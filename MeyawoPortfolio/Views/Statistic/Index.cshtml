
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/_Layout1.cshtml";
}


    <h2>İstatistikler</h2>

    <div class="alert alert-danger">Kategori Sayısı : @ViewBag.categoryCount</div>
    <div class="alert alert-warning">Proje Sayısı : @ViewBag.projectCount</div>
    <div class="alert alert-info">Mesaj Sayısı : @ViewBag.messageCount</div>
    <div class="alert alert-dark">Flutter Proje Sayısı : @ViewBag.flutterProjectCount</div>
    <div class="alert alert-primary">Okunmamıs Mesaj Sayısı : @ViewBag.isNotReadMessageCount</div>
    <div class="alert alert-success">Son Proje Adı Sayısı : @ViewBag.lastProjectName</div>
    <div class="alert alert-info">Referans Sayısı : @ViewBag.testimonialCount</div>
    <div class="alert alert-warning">Sosyal Medya Sayısı : @ViewBag.socialMediaCount</div>
    <div class="alert alert-warning">Python Proje Sayısı : @ViewBag.pythonProjectCount</div>
    <div class="alert alert-warning">React Proje Sayısı: @ViewBag.ReactProjectCount</div>


