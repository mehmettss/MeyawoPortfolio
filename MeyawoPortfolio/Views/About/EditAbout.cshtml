@model MeyawoPortfolio.Models.TblAbout
@{
    ViewBag.Title = "EditAbout";
    Layout = "~/Views/AdminLayout/_Layout1.cshtml";
}

<h2> Hakkımda Guncelleme Sayfası</h2>
<form method="post">
    <label>Hakkımda ID</label>
    <input type="text" class="table table-bordered" value="@Model.AboutID" name="AboutID" />
    <br />
    <label>Baslık</label>
    <input type="text" class="table table-bordered" value="@Model.Header" name="Header" />
    <br />
    <label>Baslık 2</label>
    <input type="text" class="table table-bordered" value="@Model.Title" name="Title" />
    <br />
    <label>Acıklama</label>
    <input type="text" class="table table-bordered" value="@Model.Description" name="Description" />
    <br />
    <label>Image uzantısı</label>
    <input type="text" class="table table-bordered" value="@Model.ImageUrl" name="ImageUrl" />
    <br />
    <button class="btn btn-info">Guncelle</button>
</form>







